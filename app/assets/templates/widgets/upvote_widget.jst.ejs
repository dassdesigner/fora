<% if (vote.isNew()) { %>
  <span class="btn-group upvote" role="group">
    <button type="button" class="btn btn-default">Upvote</button>
    <button type="button" class="btn btn-default vote-count">
      <%=model.escape('num_upvotes')%>
    </button>
  </span>
<% } else { %>
  <span class="btn-group upvote upvoted" role="group">
    <button type="button" class="btn btn-default">Upvoted</button>
    <button type="button" class="btn btn-default vote-count">
      <%=model.escape('num_upvotes')%>
    </button>
  </span>
<% }  %>
